if [ -z "$MH_PKG_PATH" ]; then
    echo "Cannot be built normally using makepkg, use 'mhpkg install' instead."
    exit 1
fi
source "$MH_PKG_SOURCE"
setup_git "github.com" "mihail-8480"
setup_package "mh" "A C library that is supposed to make my life easier." "$(mh_version)"
add_dependency "glibc"
use_cmake